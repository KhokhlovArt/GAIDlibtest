<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mks.gaidlibtest.MainActivity">

    <Button
        android:id="@+id/btn_show_original_gaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/show_original_gaid"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_show_original_gaid"
        android:layout_toLeftOf="@+id/lbl_original_gaid"
        android:layout_toStartOf="@+id/lbl_original_gaid"
        android:text="@string/original_gaid" />
    <TextView
        android:id="@+id/lbl_original_gaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:layout_below="@+id/btn_show_original_gaid"
        android:layout_centerHorizontal="true"
        />

    <Button
        android:id="@+id/btn_show_fake_gaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lbl_original_gaid"
        android:layout_marginTop="54dp"
        android:text="@string/show_fake_gaid"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_show_fake_gaid"
        android:layout_toLeftOf="@+id/lbl_fake_gaid"
        android:layout_toStartOf="@+id/lbl_fake_gaid"
        android:text="@string/fake_gaid" />
    <TextView
        android:id="@+id/lbl_fake_gaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:layout_below="@+id/btn_show_fake_gaid"
        android:layout_centerHorizontal="true"
        />

</RelativeLayout>
